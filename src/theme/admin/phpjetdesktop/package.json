{
  "name": "phpjet_admin",
  "version": "1.0.0",
  "description": "theme",
  "main": "index.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,scss",
      "runOnChangeOnly": false
    },
    "dev": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx,scss",
      "runOnChangeOnly": false
    }
  },
  "dependencies": {
    "autoprefixer": "^10.2.3",
    "bootstrap": "^4.5.3",
    "i18next": "^19.8.4",
    "neutrino": "^9.4.0",
    "npm-watch": "^0.7.0",
    "postcss-loader": "^4.2.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.5",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "watch": "^1.0.2",
    "webpack-cli": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "mini-css-extract-plugin": "^1.3.4",
    "node-sass": "^5.0.0",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.17.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "watch:dev": "npm-watch dev",
    "watch:build": "npm-watch build"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ],
      [
        "@babel/plugin-proposal-private-methods"
      ]
    ]
  },
  "browserslist": [
    "defaults",
    "last 4 versions"
  ],
  "author": "PHPJet",
  "license": "MIT"
}
